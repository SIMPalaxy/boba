func drop = {
    let n;
}

func swap = {
    let n m;
    m n
}

func counter =
    get! 1 add-isize
    put!
    get! 1 add-isize
    put!
    get! 1 add-isize

main =
    2
    handle s {
        counter
    } with {
        get! => s s resume;
        put! n => n resume;
        after => nil-list swap cons-list s snoc-list;
    }